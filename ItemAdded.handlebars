{
    "content": "{{MentionType}}",
    "avatar_url": "{{AvatarUrl}}",
    "username": "{{{BotUsername}}}",
    "embeds": [
        {   
            {{#if_equals ItemType 'Season'}}
            "author": {
                "name": "Season Added",
                "icon_url": "https://cdn.discordapp.com/attachments/832180255103385650/1084492154619039754/2503508.png"
            },
            "title": "{{{SeriesName}}}",
            "description": "{{{Overview}}}",
            {{/if_equals}}



            {{#if_equals ItemType 'Movie'}}
            "author": {
                "name": "Movie Added",
                "icon_url": "https://cdn.discordapp.com/attachments/832180255103385650/1084492154619039754/2503508.png"
            },
            "title": "{{{Name}}}",
            "description": "{{{Overview}}}",
            {{/if_equals}}



            {{#if_equals ItemType 'Episode'}}
            "author": {
                "name": "Episode Added",
                "icon_url": "https://cdn.discordapp.com/attachments/832180255103385650/1084492154619039754/2503508.png"
            },
            "title": "{{{SeriesName}}}",
            {{/if_equals}}

            "fields": [
            {{#if_equals ItemType 'Episode'}}
            {
                "name": "Name",
                "value": "{{{Name}}}",
                "inline": true
            },
            {
                "name": "Season",
                "value": "{{SeasonNumber00}}",
                "inline": true
            },
            {
                "name": "Episode",
                "value": "{{EpisodeNumber00}}",
                "inline": true
            },
            {
                "name": "Overview",
                "value": "{{{Overview}}}",
                "inline": false
            },
            {{/if_equals}}
            {
                "name": "Year",
                "value": "{{Year}}",
                "inline": false
            },
            {
                "name": "Watch Now",
                "value": "[Jellyflix]({{ServerUrl}}/web/index.html#!/details?id={{ItemId}}&serverId={{ServerId}})",
                "inline": true
            },
            {{#if_exist Provider_imdb~}}
            {
                "name": "IMDb",
                "value": "[IMDb](https://www.imdb.com/title/{{Provider_imdb}}/)",
                "inline": true
            },
            {{~/if_exist~}}
            {{~#if_exist Provider_tmdb~}}        
                {
                    "name": "TMDb",
                    {{~#if_equals ItemType 'Movie'~}}
                        "value": "[TMDb](https://www.themoviedb.org/movie/{{Provider_tmdb}})",
                    {{~else~}}
                        "value": "[TMDb](https://www.themoviedb.org/tv/{{Provider_tmdb}})",
                    {{~/if_equals~}}
                    "inline": true
                },
            {{~/if_exist~}}
            ]

            "thumbnail":{
                "url": "{{ServerUrl}}/Items/{{ItemId}}/Images/Primary"
            },

            "image": {
                "url": "{{ServerUrl}}/Items/{{ItemId}}/Images/Backdrop"
            },

            "url": "{{ServerUrl}}/web/index.html#!/details?id={{ItemId}}&serverId={{ServerId}}",
            "color": "{{EmbedColor}}",

            "footer": {
                "text": "{{{ServerName}}} ",
                "icon_url": "{{AvatarUrl}}"
            },

            "timestamp": "{{Timestamp}}"
        }
    ]
}